    <command:details>
      <command:name>Set-BackupProfileLocation</command:name>
      <command:verb>Set</command:verb>
      <command:noun>BackupProfileLocation</command:noun>
      <maml:description>
        <maml:para>To be used to set the value of a folder so MKPowerShell backs-up the sessions profile script ($PROFILE)</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Upon PowerShell startup, the session's profile script ($PROFILE) will be copied to the value set by this function.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Set-BackupProfileLocation</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="2" aliases="none">
          <maml:name>Value</maml:name>
          <maml:Description>
            <maml:para>Path to a folder</maml:para>
          </maml:Description>
